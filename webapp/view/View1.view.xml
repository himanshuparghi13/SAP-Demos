<mvc:View
    controllerName="hp.project3.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
>
    <Page
        id="pageView1"
        title="{i18n>title}"
    >
        <content>
            <HBox
                width="100%"
                justifyContent="Center"
            >
                <VBox
                    width="90%"
                    class="sapUiResponsiveMargin sapUiMediumMarginTop "
                >
                    <form:Form
                        editable="true"
                        id="form1View1"
                    >
                        <form:layout>
                            <form:ColumnLayout
                                columnsXL="4"
                                columnsL="2"
                                columnsM="1"
                                labelCellsLarge="1"
                                emptyCellsLarge="0"
                            />
                        </form:layout>
                        <form:title>
                            <core:Title text="Usage Data" />
                        </form:title>

                        <form:formContainers>
                            <form:FormContainer>
                                <form:layoutData>
                                    <form:ColumnContainerData
                                        columnsXL="4"
                                        columnsL="2"
                                        columnsM="1"
                                    />
                                </form:layoutData>
                                <form:formElements>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Client"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/usageData/client}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="User"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/usageData/user}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Language"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/usageData/language}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="1"
                                                        cellsSmall="1"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Previous Logon"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/usageData/previousLogonDate}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Input>
                                            <Input
                                                value="{systemData>/usageData/previousLogonTime}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Logon"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/usageData/logonTime}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="System time"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/usageData/systemTime}"
                                                editable="true"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                            </form:FormContainer>
                        </form:formContainers>
                    </form:Form>

                    <form:Form editable="true" id="form2View1">
                        <form:title>
                            <core:Title text="SAP data" />
                        </form:title>

                        <form:layout>
                            <form:ColumnLayout
                                columnsXL="4"
                                columnsL="2"
                                columnsM="1"
                                labelCellsLarge="1"
                                emptyCellsLarge="0"
                            />
                        </form:layout>

                        <form:formContainers>
                            <form:FormContainer>
                                <form:title>
                                    <core:Title
                                        text="Repository Data"
                                        id="titles2View1titles"
                                        class="sapUiSmallMargin"
                                    />
                                </form:title>
                                <form:layoutData>
                                    <form:ColumnContainerData columnsL="2" />
                                </form:layoutData>

                                <form:formElements>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="transactions"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/transaction}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Program (screen)"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/programScreen}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Program (Subdynpro)"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/programSubdynpro}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="program (GUI)"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/programGUI}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Screen Number"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/screenNumber1}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="5"
                                                        cellsSmall="5"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Screen Number"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/screenNumber2}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="GUI Status"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/repositoryData/guiStatus}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                            </form:FormContainer>
                            <form:FormContainer>
                                <form:title>
                                    <core:Title
                                        text="SAP System Data"
                                        id="titlesView1"
                                    />
                                </form:title>
                                <form:layoutData>
                                    <form:ColumnContainerData columnsL="2" />
                                </form:layoutData>

                                <!-- <form:toolbar>
                            <Toolbar >
                            <ToolbarSpacer ></ToolbarSpacer>
                            </Toolbar>
                        </form:toolbar> -->

                                <form:formElements>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Product Version"
                                                class=""
                                                labelFor="productVersionInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/systemData/productVersion}"
                                                id="productVersionInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="5"
                                                        cellsSmall="5"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Installation Number"
                                                class=""
                                                labelFor="installationNumberInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/systemData/installationNumber}"
                                                id="installationNumberInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Unicode System"
                                                class=""
                                                labelFor="unicodeSystemInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/systemData/unicodeSystem}"
                                                id="unicodeSystemInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement
                                        visible="true"
                                        id="hidden1View1"
                                    >
                                        <form:label>
                                            <Label
                                                text="TEmp"
                                                class=""
                                                labelFor="tempInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/systemData/temp}"
                                                id="tempInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="5"
                                                        cellsSmall="5"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="License Expires On"
                                                labelFor="licenseExpiresInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Input
                                                value="{systemData>/systemData/licenseExpires}"
                                                id="licenseExpiresInputView1"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Input>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                            </form:FormContainer>
                        </form:formContainers>
                    </form:Form>
                    <HBox
                        justifyContent="End"
                        class="sapUiSmallMarginTop"
                    >
                        <Button
                            text="Test OData"
                            type="Neutral"
                            press="onTestOData"
                            class="sapUiTinyMargin"
                        />
                        <Button
                            text="OK"
                            type="Accept"
                            press="onSave"
                            class="sapUiTinyMargin"
                        />
                       
                        <Button
                            text="clear"
                            type="Negative"
                            press="onClear"
                            class="sapUiTinyMargin"
                        />
                    </HBox>
                </VBox>
            </HBox>
        </content>
    </Page>
</mvc:View>
