<mvc:View
    controllerName="hp.project3.controller.View2"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
>
    <Page
        showNavButton="true"
        navButtonPress=".onNavBack"
        id="pageView2"
        title="{i18n>title}"
    >
        <content>
            <HBox
                width="100%"
                justifyContent="Center"
            >
                <VBox
                    width="90%"
                    class="sapUiResponsiveMargin sapUiMediumMarginTop "
                >
                    <form:Form
                        editable="true"
                        id="form1View2"
                    >
                        <form:layout>
                            <form:ColumnLayout
                                columnsXL="4"
                                columnsL="2"
                                columnsM="1"
                                labelCellsLarge="1"
                                emptyCellsLarge="0"
                            />
                        </form:layout>
                        <form:title>
                            <core:Title text="Usage Data" />
                        </form:title>

                        <form:formContainers>
                            <form:FormContainer>
                                <form:layoutData>
                                    <form:ColumnContainerData
                                        columnsXL="4"
                                        columnsL="2"
                                        columnsM="1"
                                    />
                                </form:layoutData>
                                <form:formElements>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Client"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/client}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="User"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/user}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Language"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/language}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="1"
                                                        cellsSmall="1"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Previous Logon"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/previousLogonDate}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Text>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/previousLogonTime}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Logon"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/logonTime}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="System time"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/usageData/systemTime}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                            </form:FormContainer>
                        </form:formContainers>
                    </form:Form>

                    <form:Form editable="true">
                        <form:title>
                            <core:Title text="SAP data" />
                        </form:title>

                        <form:layout>
                            <form:ColumnLayout
                                columnsXL="4"
                                columnsL="2"
                                columnsM="1"
                                labelCellsLarge="1"
                                emptyCellsLarge="0"
                            />
                        </form:layout>

                        <form:formContainers>
                            <form:FormContainer>
                                <form:title>
                                    <core:Title
                                        text="Repository Data"
                                        id="titles2View2"
                                        class="sapUiSmallMargin"
                                    />
                                </form:title>
                                <form:layoutData>
                                    <form:ColumnContainerData columnsL="2" />
                                </form:layoutData>

                                <form:formElements>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="transactions"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/transaction}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Program (screen)"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/programScreen}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Program (Subdynpro)"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/programSubdynpro}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="program (GUI)"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/programGUI}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Screen Number"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/screenNumber1}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="5"
                                                        cellsSmall="5"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Screen Number"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/screenNumber2}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="GUI Status"
                                                class=""
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/repositoryData/guiStatus}"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                            </form:FormContainer>
                            <form:FormContainer>
                                <form:title>
                                    <core:Title
                                        text="SAP System Data"
                                        id="titlesView2"
                                    />
                                </form:title>
                                <form:layoutData>
                                    <form:ColumnContainerData columnsL="2" />
                                </form:layoutData>

                                <form:formElements>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Product Version"
                                                class=""
                                                labelFor="productVersionInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/systemData/productVersion}"
                                                id="productVersionInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="5"
                                                        cellsSmall="5"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Installation Number"
                                                class=""
                                                labelFor="installationNumberInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/systemData/installationNumber}"
                                                id="installationNumberInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="Unicode System"
                                                class=""
                                                labelFor="unicodeSystemInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/systemData/unicodeSystem}"
                                                id="unicodeSystemInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="2"
                                                        cellsSmall="2"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement
                                        visible="true"
                                        id="hidden1View2"
                                    >
                                        <form:label>
                                            <Label
                                                text=""
                                                class=""
                                                labelFor="tempInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/systemData/temp}"
                                                id="tempInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="5"
                                                        cellsSmall="5"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                    <form:FormElement>
                                        <form:label>
                                            <Label
                                                text="License Expires On"
                                                labelFor="licenseExpiresInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="6"
                                                        cellsSmall="6"
                                                    />
                                                </layoutData>
                                            </Label>
                                        </form:label>
                                        <form:fields>
                                            <Text
                                                text="{globalData>/currentEntry/systemData/licenseExpires}"
                                                id="licenseExpiresInputView2"
                                            >
                                                <layoutData>
                                                    <form:ColumnElementData
                                                        cellsLarge="3"
                                                        cellsSmall="3"
                                                    />
                                                </layoutData>
                                            </Text>
                                        </form:fields>
                                    </form:FormElement>
                                </form:formElements>
                            </form:FormContainer>
                        </form:formContainers>
                    </form:Form>
                    <HBox
                        justifyContent="End"
                        class="sapUiSmallMarginTop"
                    >
                        <Button
                            text="OK"
                            type="Accept"
                            press="onSave"
                            class="sapUiTinyMargin"
                        />
                        <Button
                            text="Back"
                            type="Back"
                            press="onNavBack"
                            class="sapUiTinyMargin"
                        />
                    </HBox>
                </VBox>
            </HBox>
        </content>
    </Page>
</mvc:View>
