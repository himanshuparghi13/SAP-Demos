{
  "_version": "1.81.0",
  "sap.app": {
    "id": "hp.project3",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.2",
      "toolsId": "4f1035fb-74ef-4b40-aaf3-b63562bd532e"
    },
    "dataSources": {
      "mainService": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.144.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.viz": {},
        "sap.f": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "hp.project3.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "useBatch": true,
          "disableHeadRequestForToken": true
        }
      },
      "Products": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Products.json"
      },
      "Population": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Population.json"
      },
      "Populations": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Populations.json"
      },
      "Languages": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Languages.json"
      },
      "data": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/data.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "hp.project3.view",
        "async": true,
        "bypassed": {
          "target": ["notFound"]
        }
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": ":?query:",
          "target": ["TargetView1"]
        },
        {
          "name": "RouteView2",
          "pattern": "RouteView2",
          "target": ["TargetView2"]
        },
        {
          "name": "RouteView3",
          "pattern": "RouteView3",
          "target": ["TargetView3"]
        },
        {
          "name": "RouteODataTest",
          "pattern": "odata-test",
          "target": ["TargetODataTest"]
        },
        {
          "name": "RouteTable",
          "pattern": "table",
          "target": ["TargetTable"]
        },
        {
          "name": "RouteNewTable",
          "pattern": "newTable",
          "target": ["TargetNewTable"]
        },
        {
          "name": "RouteCharts",
          "pattern": "charts",
          "target": ["TargetCharts"]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
        "TargetView2": {
          "id": "View2",
          "name": "View2"
        },
        "TargetView3": {
          "id": "View3",
          "name": "View3"
        },
        "TargetODataTest": {
          "id": "ODataTest",
          "name": "ODataTest"
        },
        "TargetTable": {
          "id": "Table",
          "name": "Table"
        },
        "TargetNewTable": {
          "id": "NewTable",
          "name": "NewTable"
        },
        "TargetCharts": {
          "id": "Charts",
          "name": "Charts"
        }
      }
    },
    "rootView": {
      "viewName": "hp.project3.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
